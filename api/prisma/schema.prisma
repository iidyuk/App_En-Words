generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Word {
  id        Int      @id @default(autoincrement())
  term      String
  meaning   String
  example   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
